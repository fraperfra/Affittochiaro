{
  "name": "affittochiaro-backend",
  "version": "1.0.0",
  "description": "Affittochiaro Backend - Serverless AWS Lambda Functions",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint 'lambdas/**/*.ts'",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:rollback": "ts-node scripts/migrate.ts rollback",
    "seed": "ts-node scripts/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.470.0",
    "@aws-sdk/client-dynamodb": "^3.470.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/client-ses": "^3.470.0",
    "@aws-sdk/client-sesv2": "^3.980.0",
    "@aws-sdk/client-sqs": "^3.470.0",
    "@aws-sdk/lib-dynamodb": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "stripe": "^14.10.0",
    "twilio": "^5.12.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "esbuild": "^0.19.9",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "serverless": "^3.37.0",
    "serverless-esbuild": "^1.50.1",
    "serverless-offline": "^13.3.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
